    "eslint-plugin-prettier": "3.1.2",
    "fs-extra": "8.1.0",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "mocha": "6.2.2",
    "prettier": "1.19.1",
    "yeoman-assert": "3.1.1",
    "yeoman-test": "2.0.0"
  },
  "scripts": {
    "prettier:format": "prettier --write \"{,**/}*.{js,json,md,yml}\"",
